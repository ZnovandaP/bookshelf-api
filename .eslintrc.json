{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb/base", "airbnb-typescript/base"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "import/no-extraneous-dependencies": "off",
    "@typescript-eslint/no-unused-vars": "warn"
  },
  "ignorePatterns": ["dist", "node_modules"]
}
